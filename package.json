{
  "name": "illusion-website",
  "version": "0.0.0",
  "description": "The official website of illusion.",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run local next dev",
    "build": "concurrently -g \"npm:build:*\"",
    "build:clean": "rimraf .next",
    "build:next": "next build",
    "build:adjust": "rimraf -g \"out/**/*/__EMPTYSLUGS.*\"",
    "preview": "concurrently -g \"npm:preview:*\"",
    "preview:build": "bun run build",
    "preview:serve": "serve out",
    "prod": "cross-env NEXT_PUBLIC_BASE_URL=\"https://www.ayanamist.blog\" bun run",
    "local": "cross-env NEXT_PUBLIC_BASE_URL=\"http://localhost:3000\" bun run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/illusionDev-2/illusion-website.git"
  },
  "keywords": [
    "illusion"
  ],
  "author": "illusionDev-2",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/illusionDev-2/illusion-website/issues"
  },
  "homepage": "https://github.com/illusionDev-2/illusion-website#readme",
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.1.1",
    "rimraf": "^5.0.5",
    "serve": "^14.2.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "cheerio": "^1.0.0-rc.12",
    "gray-matter": "^4.0.3",
    "html-to-text": "^9.0.5",
    "iter-tools": "^7.5.3",
    "lizod": "^0.2.7",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-budoux": "^1.0.0",
    "rehype-react": "^8.0.0",
    "rehype-shift-heading": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-rehype": "^11.1.0",
    "simple-git": "^3.24.0"
  }
}